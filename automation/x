  <script src="https://covid19.nagaland.gov.in/js/jquery.Cookie.js"></script>

  <script src="https://covid19.nagaland.gov.in/js/fontAc.js"></script>

  <script type="text/javascript">
  $(document).ready(function(){
      var conState = "0";
      if($.cookie('conState') == '0' || 'conState' == null){
        $("*").css("background-color","black");
        $("*").css("color","white");
        conState = "1";
      }
      $("#hi-contrast").click(function(){
      if(conState=="0"){
        $("*").css("background-color","black");
        $("*").css("color","white");
        conState = "1";
        $.cookie('conState' ,'0');
      }else{
        $("*").css("background-color","");
        $("*").css("color","");
        conState = "0";
        $.cookie('conState' ,'1');
      }
    });
  });
  </script>
</body>

</html>
