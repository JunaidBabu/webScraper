var myLineChart = new Chart(ctx, {
  type: 'line',
  data:{
    labels: ['24-05-2020','25-05-2020','26-05-2020','27-05-2020','28-05-2020','29-05-2020','30-05-2020','31-05-2020','01-06-2020','02-06-2020','03-06-2020','04-06-2020',],
